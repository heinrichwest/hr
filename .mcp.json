{
  "mcpServers": {
    "shadcn": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "shadcn@latest",
        "mcp"
      ]
    },
    "firebase": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "firebase-tools@latest",
        "experimental:mcp"
      ]
    },
    "chrome-devtools": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "chrome-devtools-mcp@latest"
      ]
    }
  }
}
